#: ../carrick/carrick-applet.c:63
#, c-format
msgid "Offline"
msgstr "Fuera de línea"

#: ../carrick/carrick-applet.c:64
#, c-format
msgid "No active connection."
msgstr "No hay conexión activa."

#: ../carrick/carrick-applet.c:70
#, c-format
msgid "Online"
msgstr "En línea"

#: ../carrick/carrick-applet.c:75
#, c-format
msgid "Now online and connected to %s."
msgstr "Está ahora en línea y conectado a %s."

#: ../carrick/carrick-pane.c:283
msgid "New connection settings"
msgstr "Nueva configuración de conexión"

#: ../carrick/carrick-pane.c:319
msgid ""
"Enter the name of the network you want\n"
"to connect to and, where necessary, the\n"
"password and security type."
msgstr ""
"Ingrese el nombre de la red a la que desea\n"
"conectarse y, si es necesario, la contraseña\n"
"y el tipo de seguridad."

#: ../carrick/carrick-pane.c:329
msgid "Network name"
msgstr "Nombre de red"

#: ../carrick/carrick-pane.c:341
msgid "Network security"
msgstr "Seguridad de red"

#: ../carrick/carrick-pane.c:365
msgid "Password"
msgstr "Contraseña"

#: ../carrick/carrick-pane.c:380
#: ../carrick/carrick-service-item.c:855
msgid "Show password"
msgstr "Mostrar contraseña"

#: ../carrick/carrick-pane.c:791
msgid "Networks"
msgstr "Redes"

#: ../carrick/carrick-pane.c:830
msgid "Add new connection"
msgstr "Agregue una nueva conexión"

#: ../carrick/carrick-pane.c:856
msgid "WiFi"
msgstr "Wi-Fi"

#: ../carrick/carrick-pane.c:879
#: ../carrick/carrick-service-item.c:196
msgid "Wired"
msgstr "Cableado"

#: ../carrick/carrick-pane.c:905
msgid "3G"
msgstr "3G"

#: ../carrick/carrick-pane.c:930
msgid "WiMAX"
msgstr "WiMAX"

#: ../carrick/carrick-pane.c:964
msgid "Offline mode"
msgstr "Modo fuera de línea"

#: ../carrick/carrick-pane.c:988
msgid "This will disable all your connections"
msgstr "Esto deshabilitará todas sus conexiones"

#: ../carrick/carrick-service-item.c:163
#, c-format
msgid "%s encrypted"
msgstr "%s cifrado"

#: ../carrick/carrick-service-item.c:208
msgid "Disconnect"
msgstr "Desconectarse"

#: ../carrick/carrick-service-item.c:211
msgid "Connected"
msgstr "Conectado"

#: ../carrick/carrick-service-item.c:218
#, c-format
msgid "Connecting"
msgstr "Conectandose"

#: ../carrick/carrick-service-item.c:221
msgid "Configuring"
msgstr "Configurando"

#: ../carrick/carrick-service-item.c:232
#: ../carrick/carrick-service-item.c:244
#: ../carrick/carrick-service-item.c:844
msgid "Connect"
msgstr "Conectarse"

#: ../carrick/carrick-service-item.c:247
msgid "Connection failed"
msgstr "La conexión falló"

#: ../carrick/carrick-service-item.c:254
#: ../carrick/carrick-service-item.c:257
#, c-format
msgid "Disconnecting"
msgstr "Desconectandose"

#: ../carrick/carrick-service-item.c:306
msgid "Type password here"
msgstr "Ingrese la contraseña aquí"

#: ../carrick/carrick-service-item.c:383
msgid "Really remove?"
msgstr "¿Remover realmente?"

#: ../carrick/carrick-service-item.c:401
#, c-format
msgid ""
"Do you want to remove the %s %s network? This\n"
"will forget the password and you will no longer be\n"
"automatically connected to %s."
msgstr ""
"¿Desea remover la red %s %s? Esta acción\n"
"olvidará la contraseña y usted no volverá a estar\n"
"conectado automáticamente a %s."

