#: ../carrick/carrick-pane.c:316
msgid "New connection settings"
msgstr "Paramètres de la nouvelle connexion"

#: ../carrick/carrick-pane.c:352
msgid ""
"Enter the name of the network you want\n"
"to connect to and, where necessary, the\n"
"password and security type."
msgstr ""
"Saisissez le nom du réseau auquel vous\n"
"voulez vous connecter et, si nécessaire\n"
"le mot de passe et type de sécurité."

#: ../carrick/carrick-pane.c:362
msgid "Network name"
msgstr "Nom de réseau"

#: ../carrick/carrick-pane.c:374
msgid "Network security"
msgstr "Securité du réseau"

#: ../carrick/carrick-pane.c:398
msgid "Password"
msgstr "Mot de passe"

#: ../carrick/carrick-pane.c:413
#: ../carrick/carrick-service-item.c:961
msgid "Show password"
msgstr "Montrer le mot de passe"

#: ../carrick/carrick-pane.c:755
msgid "Networks"
msgstr "Réseaux"

#: ../carrick/carrick-pane.c:799
msgid "Add new connection"
msgstr "Ajouter une nouvelle connexion"

#: ../carrick/carrick-pane.c:826
msgid "WiFi"
msgstr "WiFi"

#: ../carrick/carrick-pane.c:855
#: ../carrick/carrick-service-item.c:201
msgid "Wired"
msgstr "Par câble"

#: ../carrick/carrick-pane.c:884
msgid "3G"
msgstr "3G"

#: ../carrick/carrick-pane.c:913
msgid "WiMAX"
msgstr "WiMAX"

# "Offline" lack of space
#: ../carrick/carrick-pane.c:953
msgid "Offline mode"
msgstr "Hors-ligne"

# "\n" because of truncation 
#: ../carrick/carrick-pane.c:978
msgid "This will disable all your connections"
msgstr ""
"Ceci va désactiver toutes\n"
"vos connexions"

#: ../carrick/carrick-service-item.c:168
#, c-format
msgid "%s encrypted"
msgstr "%s encrypté"

#: ../carrick/carrick-service-item.c:235
msgid "Disconnect"
msgstr "Déconnecter"

#: ../carrick/carrick-service-item.c:238
msgid "Connected"
msgstr "Connecté"

#: ../carrick/carrick-service-item.c:245
#, c-format
msgid "Connecting"
msgstr "En cours de connexion"

#: ../carrick/carrick-service-item.c:248
msgid "Configuring"
msgstr "En cours de configuration"

#: ../carrick/carrick-service-item.c:257
#: ../carrick/carrick-service-item.c:267
#: ../carrick/carrick-service-item.c:950
msgid "Connect"
msgstr "Connecter"

#: ../carrick/carrick-service-item.c:270
